<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <filter>
        <filter-name>CharacterEncodingFilter</filter-name>
        <filter-class>com.bpps.servlet.CharacterEncodingFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>CharacterEncodingFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>action</servlet-name>
        <servlet-class>org.apache.struts.action.ActionServlet</servlet-class>
        <init-param>
            <param-name>config</param-name>
            <param-value>/WEB-INF/struts-config.xml</param-value>
        </init-param>
        <init-param>
            <param-name>debug</param-name>
            <param-value>2</param-value>
        </init-param>
        <init-param>
            <param-name>detail</param-name>
            <param-value>2</param-value>
        </init-param>
        <load-on-startup>2</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>ImageSearchServlet</servlet-name>
        <servlet-class>com.bpps.servlet.ImageSearchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisSymServlet</servlet-name>
        <servlet-class>com.bpps.servlet.DisSymServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>multiAttrAssociate</servlet-name>
        <servlet-class>com.bpps.servlet.MultiAttrAssociate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DiseaseImageSearch</servlet-name>
        <servlet-class>com.bpps.servlet.DiseaseImageSearch</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>action</servlet-name>
        <url-pattern>*.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImageSearchServlet</servlet-name>
        <url-pattern>/ImageSearchServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisSymServlet</servlet-name>
        <url-pattern>/disSymServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>multiAttrAssociate</servlet-name>
        <url-pattern>/multiAttrAssociate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DiseaseImageSearch</servlet-name>
        <url-pattern>/diseaseImageSearch</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
